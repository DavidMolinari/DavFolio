<section id="interests" class="section section-interests">
    <div class="animate-up">
        <h2 class="section-title">Mes centres d'interêt</h2>

        <div class="section-box">
            <p></p>

            <ul class="interests-list">
                <li>
                    <i class="map-icon map-icon-airport"></i>
                    <span>Voyager</span>
                </li>
                <li>
                    <i class="map-icon map-icon-movie-theater"></i>
                    <span>Regarder des films</span>
                </li>
                <li>
                    <i class="map-icon map-icon-cafe"></i>
                    <span>Le café !</span>
                </li>
                <li>
                    <i class="map-icon map-icon-bar"></i>
                    <span>Sortir</span>
                </li>
                <li>
                    <i class="map-icon map-icon-search"></i>
                    <span>Développer</span>
                </li>
                <li>
                    <i class="map-icon map-icon-pet-store"></i>
                    <span>Les chats !</span>
                </li>
                <li>
                    <i class="map-icon map-icon-swimming"></i>
                    <span>Swimming</span>
                </li>
            </ul>
        </div>
    </div>
</section><!-- #interests -->


<section id="clients" class="section section-clients">
    <div class="animate-up">

        <div class="clients-carousel">

            {% for interet in interets  %}
                <div class="client-logo">
                    <img src={{ asset('Assets/img/logos/'~ interet.nomFichier ~'.svg') }} title="{{ interet.libelle }}" alt="{{ interet.libelle }}"/>
                </div>
            {% endfor %}

        </div>
    </div>
</section><!-- #clients -->
